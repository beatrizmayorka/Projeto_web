<!DOCTYPE html>
<html>

<head>
  <title>Administration in projects</title>
  <%- include('../../imports'); -%>
    <link rel="shortcut icon" href=" ">
</head>

<body>
  <header>
    <%- include('../../navbar'); -%>
  </header>

  <div class="container">
    <div class="row">
      <h1>Administration de projects</h1>
    </div>

    <div class="row mb-2">
      <a href="/admin/projects/create" class="btn btn-dark btn-outline-info"> New Project </a>
    </div>

    <div class="row">
      <table class="table table-hover ">
        <thead class="thead-dark">
          <tr>
            <th scope="col">Code</th>
            <th scope="col">Title</th>
          </tr>
        </thead>
        <tbody class="table-active">
          <% projects.forEach((project)=> { %>
            <tr>
              <td>
                <%= project.id %>
              </td>
              <td>
                <%= project.name %>
              </td>
            </tr>
            <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</body>

</html>